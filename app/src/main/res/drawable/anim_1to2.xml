<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:drawable="@drawable/drawable_1">

    <target
        android:animation="@anim/middle_to_right_up"
        android:name="right_up" />
    <target
        android:animation="@anim/middle_to_left_down"
        android:name="left_down" />

    <target
        android:animation="@anim/middle_disappear"
        android:name="middle" />
    <target
        android:animation="@anim/middle_disappear"
        android:name="left_up" />
    <target
        android:animation="@anim/middle_disappear"
        android:name="left_middle" />
    <target
        android:animation="@anim/middle_disappear"
        android:name="right_middle" />
    <target
        android:animation="@anim/middle_disappear"
        android:name="right_down" />

</animated-vector>